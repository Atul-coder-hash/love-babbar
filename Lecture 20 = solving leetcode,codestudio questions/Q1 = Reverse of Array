                                                    // Reverse of an Array without using vector 

#include<iostream>
using namespace std;




void reverse(int a[],int size)
{
    int s=0;
    int e= size-1;
    while(s<=e)
    {
     swap(a[s],a[e]);
     s++;
     e--;
    }

}

void print(int a[],int size)
{
    
   for(int i=0;i<size;i++)
   {
    cout<<a[i]<<"\t";
   }

}

int main()
{
   int a[]={1,3,5,7,9,19};
  
   int size = sizeof(a)/sizeof(int);
   
reverse(a,size);
print(a,size);

}
