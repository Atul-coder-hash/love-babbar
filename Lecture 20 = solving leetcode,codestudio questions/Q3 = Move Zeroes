#include<iostream>
using namespace std;

int main()
{
    int a[]={2,0,1,3,0,0,0};
    int size = sizeof(a)/sizeof(int);
    int i=0;
    int j=0;

    for(int j=0;j<size;j++)
    {
        if(a[j]!=0)
        {
            swap(a[i],a[j]);
            i++;
        }
    }

   

    for(int k=0;k<size;k++)
    {
        cout<<a[k];
    }
}
