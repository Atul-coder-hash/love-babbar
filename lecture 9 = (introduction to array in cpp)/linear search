#include<iostream>
using namespace std;

bool check(int a[],int size, int key)
{
    for(int i=0;i<size;i++)
    {
        if(a[i]==key)
        {
            return 1;
        }
        
    }
    return 0;
}

int main(){

int a[]={1,2,4,3,7,-7,10};
int key;
cout<<"enter element you want to search:-"<<endl;
cin>>key;

bool found=check(a,7,key);

if(found)
{
    cout<<"present ";
}
else{
    cout<<"not present";
}



}
