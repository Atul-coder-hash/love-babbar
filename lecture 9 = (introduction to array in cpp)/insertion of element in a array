#include<iostream>
using namespace std;

int main()
{
int a[6]={10, 20, 30, 40, 50};
int size =6;

int n=25;
int ind=2;

// shifting value 
for(int i=size-1;i>ind;i--){
    
    a[i]=a[i-1];

}
    a[ind]=n;// inserting value at given index
    
    
for(int i=0;i<size;i++)
{
    cout<<a[i]<<" ";
}
   
}
