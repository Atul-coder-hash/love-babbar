
#include<iostream>
using namespace std;

class node{
  
    public:
    int data;
    node *next;      // pointer
    
    // constructor
    node(int data)
    {
           this->data = data;
           this->next= NULL;
           
    }

};


int main() {

node *node1 = new node(10);    // dynamic allocation of object
cout<<node1->data<<endl;
cout<<node1->next<<endl;
       
       
       
       
       
}
