#include<iostream>
using namespace std;

void notes(int n)
{
int count100=0,count50=0,count20=0,count10=0;

switch(1){
    case 1:count100=n/100;
            n=n%100;
             cout<<"notes of 100:-"<<count100<<endl;
     case 2:count50=n/50;
            n=n%50;
             cout<<"notes of 50:-"<<count50<<endl;
     case 3:count20=n/20;
            n=n%20;
            cout<<"notes of 20:-"<<count20<<endl;

     case 4:count10=n/10;
            n=n%10;
            cout<<"notes of 10:-"<<count10<<endl;
            break;

    default:cout<<"unexpected error";
}}
int main()
{int n;
    cout<<"enter amount:-";
    cin>>n;
    notes(n);}
