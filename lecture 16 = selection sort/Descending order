#include<iostream>
using namespace std;

void print(int a[],int size)
{
    for(int i=0;i<size;i++)
    {
        cout<<a[i]<<" ";
    }
}

void sort(int a[],int size)
{
for(int i=0;i<size-1;i++)
{
    int max = i;
    for(int j=i+1;j<size;j++)
    {
        if(a[j]>a[max])
        {max=j;

        }
    }
    swap(a[max],a[i]);
}
print(a,size);
}
int main(){
    int a[]={64,20,72,11,13,9};
   int size = sizeof(a)/sizeof(int);
   sort(a,size);
}
