#include<iostream>
using namespace std;

bool checkpalindrome(char ch[],int count)
{
     int s=0;  
     int e= count-1;
     
    while(s<=e)
    {
           if(ch[s]!=ch[e])
           {
                  return 0;
           }
           else
           {
                  s++;
                  e--;
           }
           return 1;
           
    }
}

int main()
{
       char ch[10];
       cout<<"enter:-";
       cin>>ch;
       
       int count=0;
        int i=0;
        while(ch[i]!='\0')
    {
           count++;
           i++;
           
    }
    if(checkpalindrome(ch,count)==1)
    {
           cout<<"Palindrome";
    }
    else
    {
           cout<<"Not palindrome";
    }
    
    
}
