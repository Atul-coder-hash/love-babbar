#include<iostream>
using namespace std;

int main()
{
    char a[10];
    cout<<"enter name ";
    cin>>a;
    
    
    int i=0;
    while(a[i]!='\0')
    {
        i++;
    }


    int s=0;
    int e= i-1;

    while(s<=e)
    {
        swap(a[s],a[e]);
        s++;
        e--;
    }

   cout<<a;


}
