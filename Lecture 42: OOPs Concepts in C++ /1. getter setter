#include<iostream>
using namespace std;

class Hero {
    
    //properties
    private:
    int health;

    public:
 
    char level;
  
       void print(){
              cout<<level<<endl;
       }


    int getHealth() {       // create a get health to get the value of health
        return health;
    }

    char getLevel() {
        return level;
    }

    void setHealth(int h) {
        health = h;
    }

    void setLevel(char ch) {
        level = ch;
    }

    
};



int main() {

   Hero ramesh;
   
   cout<<"Ramesh health is"<<ramesh.getHealth()<<endl;  // used to get the value of health
   
   ramesh.setHealth(70);     // create a set health to get the value of health
   ramesh.level ='A';
   
   cout<<"Health is:"<<ramesh.getHealth()<<endl;
   cout<<"Level is:"<<ramesh.level<<endl;


    return 0;
}
