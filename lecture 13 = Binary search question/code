#include<iostream>
using namespace std;

int firstocc(int a[],int size,int key)
{int start =0,end =size-1,ans=0;

    int mid = start+(end-start)/2;
    while(start<=end)
    {
    

        if(a[mid]==key)
        {
            ans = mid  ;
            end = mid-1;
            
        }
        else if(key>a[mid])
        {
            start = mid+1;

        }
        else if(key<a[mid])
        {
            end = mid-1;
        }
        mid = start+(end-start)/2;
        
        }
        return ans;

}

int main()
{
    int a[]={1,2,3,5,5};
    int c;
    cout<<"enter element:-";
    cin>>c;
    int size = sizeof(a)/sizeof(int);
    firstocc(a,size,c);
    cout<<firstocc;
}
