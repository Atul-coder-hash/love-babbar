#include<iostream>
using namespace std;

void notes(int n)
{
    if(n>=100)
    {
       int x= n/100;
        n=n-x*(100);
        cout<<"notes of 100:-"<<x<<endl;
        
    }
    
    
    if(n>=50)
    {
        int y = n/50;
        n=n-y*50;
        cout<<"notes of 50:-"<<y<<endl;

    }

     if(n>=20)
    {
        int z = n/20;
        n=n-z*20;
        cout<<"notes of 20:-"<<z<<endl;

    }
     if(n>=10)
    {
        int a = n/10;
        n=n-a*10;
        cout<<"notes of 10:-"<<a<<endl;

    }
}
int main()
{int a;
    cout<<"enter amount:-";
    cin>>a;
    notes(a);
    
}
