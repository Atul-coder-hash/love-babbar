#include<iostream>
using namespace std;


bool isprime(int n)
{
    if(n%2==0||n==2)
    {
        return 0;
    }
return 1;
}
int main()
{
    int a;
    cout<<"enter a number:-";
    cin>>a;
    if(isprime(a)==1)
    {
        cout<<"Prime number"<<endl;
    }
    else
    {
        cout<<"not prime number"<<endl;
    }
    
}
