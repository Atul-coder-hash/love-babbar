#include<iostream>
using namespace std;

int sumarray(int a[],int size)
{int sum=0;
    for(int i=0;i<size;i++)
{
    sum = sum+a[i];
}
return sum;
}

int main()
{
    int a[]={1,5,6,8,7,9};
    int size = sizeof(a)/sizeof(int);
    sumarray(a,size);
    cout<<"sum of all elements of array:-"<<sumarray(a,size);

}
