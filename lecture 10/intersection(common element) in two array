#include <iostream>
using namespace std;


void intersection(int a[],int b[], int size) {

    int c;
    for(int i=0;i<size;i++)
    {
        for(int j=0;j<size;j++)
        {
            if(a[i]==b[j])
            {
               cout<<a[i]<<endl;
            }
        }
    }

}

int main() {
    int a[] = {1,2,3,4};
    int b[]={2,4,6,8};
    int size = sizeof(a) / sizeof(int);
    intersection(a,b, size);
    return 0;
}
