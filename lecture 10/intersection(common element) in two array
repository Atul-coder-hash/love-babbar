#include <iostream>
using namespace std;


void printarray(int a[],int b[], int size) {
    for (int i = 0; i < size; i++) {
        cout << a[i] << " ";
    }
    cout << endl; 
    for (int j = 0; j < size; j++) {
        cout << b[j] << " ";
    } 
}

void intersection(int a[],int b[], int size) {

    int c[4]={0};
    for(int i=0;i<size;i++)
    {
        for(int j=0;j<size;j++)
        {
            if(a[i]==b[j])
            {
                cin>>c[j];
                
            }
        }
    }

    for (int i = 0; i < size; i++) {
        cout << c[i] << " ";
    }


    printarray(a,b, size);
     
}

int main() {
    int a[] = {1,2,3,4};
    int b[]={2,4,6,8};
    int size = sizeof(a) / sizeof(int);
    
    
    cout<<"original array:-"<<endl;
    printarray(a,b,size);


    intersection(a,b, size);

    

    return 0;
}
