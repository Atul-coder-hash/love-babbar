#include<iostream>
using namespace std;

int main()
{
       
       int a[3][3];
       
       
       for(int i=1;i<=3;i++)
       {
              for(int j=1;j<=3;j++)
              {cout<<"enter elements:-";
                     cin>>a[i][j];
              }
       }
       
       cout<<"output of main input:-"<<endl;
       for(int i=1;i<=3;i++)
       {
              for(int j=1;j<=3;j++)
              {
                     cout<<a[i][j];
              }
              cout<<"\n";
       }
       
       cout<<"Output after 90 degree rotation:-"<<endl;
        for(int i=1;i<=3;i++)
       {
              for(int j=3;j>=1;j--)
              {
                     cout<<a[j][i];
              }
              cout<<"\n";
       }
}
