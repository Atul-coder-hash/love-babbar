#include<iostream>
using namespace std;

int factorial(int n){
    
    if(n==0)   // base case
    {
        return 1;
        
    }
    int smallproblem = factorial(n-1);   // recursive relation 
    int bigproblem = n*smallproblem;  // recursive relation
  
return bigproblem;
}

int main()
{
    int n;
    
    cout<<"enter n:-";
    cin>>n;
    
    cout<<factorial(n);
    
}
